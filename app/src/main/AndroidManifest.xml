<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/playstore"
        android:label="@string/app_name"
        android:roundIcon="@drawable/playstore"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cart_Activity"
        android:screenOrientation="portrait"
        tools:targetApi="31">
        <activity
            android:name="com.mart.cart_activity.Activity.SearchResultActivity"
            android:screenOrientation="portrait"
            android:exported="false"></activity>
        <activity
            android:name="com.mart.cart_activity.Activity.ProductTypeActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.infinity_product"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.NotificationDetailsActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.SingleProductCategories"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.ProductCategoriesActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.NotificationActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.EditProfileActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.PolicyActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.OtpActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.NumberActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mart.cart_activity.Activity.ForgetPassActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.SignupActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.LoginActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.mart.cart_activity.Activity.ProfileActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.ExplorerActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.WishlistActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.CartActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.DetailActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter android:autoVerify="true">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

<!--                <data android:scheme="http" />-->
<!--                <data android:host="test.njoymart.in" />-->
<!--                <data android:pathPattern="/AppData" />-->
                <data
                    android:host="njoymart.in"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mart.cart_activity.Activity.MainActivity"
            android:screenOrientation="portrait"
            android:exported="true" />
    </application>

</manifest>