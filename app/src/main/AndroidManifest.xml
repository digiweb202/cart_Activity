            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.EditProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.PolicyActivity"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.OtpActivity"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.NumberActivity"
            android:exported="false" />
        <activity
            android:name="com.mart.cart_activity.Activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

